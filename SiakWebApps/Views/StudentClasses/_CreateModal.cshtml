@model SiakWebApps.Models.StudentClass

<form asp-action="Create" id="createStudentClassForm">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="SiswaId" class="control-label"></label>
        <input asp-for="SiswaId" class="form-control" />
        <span asp-validation-for="SiswaId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="KelasId" class="control-label"></label>
        <input asp-for="KelasId" class="form-control" />
        <span asp-validation-for="KelasId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="SemesterId" class="control-label"></label>
        <input asp-for="SemesterId" class="form-control" />
        <span asp-validation-for="SemesterId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="TanggalMasuk" class="control-label"></label>
        <input asp-for="TanggalMasuk" class="form-control" />
        <span asp-validation-for="TanggalMasuk" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="TanggalKeluar" class="control-label"></label>
        <input asp-for="TanggalKeluar" class="form-control" />
        <span asp-validation-for="TanggalKeluar" class="text-danger"></span>
    </div>
    <div class="form-group form-check">
        <label class="form-check-label">
            <input class="form-check-input" asp-for="IsActive" /> @Html.DisplayNameFor(model => model.IsActive)
        </label>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
    </div>
</form>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}